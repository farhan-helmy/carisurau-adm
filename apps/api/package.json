    "name": "Farhan Helmy",
    "email": "farhanhlmy@gmail.com"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node build/index.js",
    "build": "tsc",
    "prod": "ts-node -r dotenv/config src/index.ts",
    "dev": "concurrently \"nodemon\"",
    "dev:local": "nodemon -r dotenv/config dotenv_config_path=.env.local src/index.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node -r dotenv/config src/index.ts"
  },
  "packageManager": "yarn",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "@types/nodemailer": "^6.4.8",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^8.1.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.370.0",
    "@paralleldrive/cuid2": "^2.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.1.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "knex": "^2.4.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.0",
    "serverless-http": "^3.2.0",
    "swagger-ui-express": "^4.6.3",
    "tsoa": "^5.1.1",
    "zod": "^3.21.4"
  }
}
